<h1>POST Request Received</h1>
    {{#if bodyList.length}}
         <h2>Contents</h2>
         <table>
               <tr>
                     <th>Name</th>
                     <th>Value</th>
               </tr>
               {{#each bodyList}}
                       <tr>
                            <td>{{this.name}}</td>
                            <td>{{this.value}}</td>
                       </tr>
                {{/each}}
         </table>
  {{/if}}
