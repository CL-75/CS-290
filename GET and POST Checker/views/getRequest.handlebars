<h1>GET Request Received</h1>
<table>
      {{#if list.length}}
           <tr>
               <th>Name</th>
               <th>Value</th>
           </tr>
      {{/if}}
       {{#each list}}
              <tr>
                  <td>{{this.name}}</td>
                  <td>{{this.value}}</td>
              </tr>
       {{/each}}
</table>
